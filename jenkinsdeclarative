pipeline {
    agent any
    stages ( "clone" ) {
        steps {
            https://github.com/chandushine/spring-petclinic.git
        }
    }
    stages ( "build" ){
        steps {
            mvn package
        }
    }
}
        


pipeline {
    agent { label 'hello_world' }
    stages {
        stage('clone') {
            steps {
                git url: 'https://github.com/chandushine/spring-petclinic.git',
                branch: 'main'
            }
        }
        stage('build') {
            steps {
                sh 'mvn package'
            }
        }
    }
}